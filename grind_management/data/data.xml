<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequences for all seq in underwriting section-->
         <record id="sale_order_number_sequence" model="ir.sequence">
            <field name="name">Sales Order Number</field>
            <field name="code">sale_order_number.seq</field>
            <field name="prefix">SALES/%(year)s/%(month)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="grind_inventory_name_sequence" model="ir.sequence">
            <field name="name">Grind Inventory Name</field>
            <field name="code">grind_inventory_name.seq</field>
            <field name="prefix">MOVEMENT/%(year)s/%(month)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>